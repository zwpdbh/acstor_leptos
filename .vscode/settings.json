{
  // "rust-analyzer.cargo.features": [
  //   "ssr"
  // ],
  "rust-analyzer.procMacro.ignored": {
    "leptos_macro": [
      // optional:
      // "component",
      "server"
    ],
  },
  "rust-analyzer.rustfmt.overrideCommand": [
    "leptosfmt",
    "--stdin",
    "--rustfmt"
  ],
  // For RUST Leptos
  "files.associations": {
    "*.rs": "rust",
    "*.heex": "phoenix-heex",
    "*.eex": "phoenix-heex"
  },
  "tailwindCSS.includeLanguages": {
    "rust": "html",
    "phoenix-heex": "html",
    "elixir": "html",
  },
  "tailwindCSS.experimental.classRegex": [
    "class: \"(.*)\""
  ],
  "emmet.showAbbreviationSuggestions": true,
  "emmet.includeLanguages": {
    "rust": "html",
    "phoenix-heex": "html",
    "html-eex": "html"
  },
  "editor.quickSuggestions": {
    "comments": "inline",
    "strings": "on",
    "other": "on"
  },
}